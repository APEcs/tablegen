            <data for="week">{T_[weeknum]}</data>
            <data for="step">Registration {T_[weekdate]}-{T_[weekdate+4]}</data>
            <data for="tut">Meet tutor for lunch on {T_[weekday+2]}</data>
            <data for="lab"><![CDATA[<a href="{L_[Phase0]}">Welcome Lab, Intro Lab 1</a>]]></data>
            <data for="lec"><![CDATA[<a href="{L_[Phase0]}">Intro talks</a>]]></data>
            <data for="other"><![CDATA[<a href="{L_[Phase0]}">Workshop</a>]]></data>
        </row>

