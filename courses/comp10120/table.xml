<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- Format information:

-->
<course>
    <style>
   table.schedule {
     border: 1px black solid;
     border-spacing: 0px;
     border-collapse: collapse;
   }
   table.schedule td, table.schedule th {
     border: 1px solid black;
     font-family: arial, verdana, sans-serif;
     font-size: 12px;
     letter-spacing: 0;
     line-height: 1.5em;
     vertical-align: top;
     padding: 2px;
   }
   table.schedule td a, table.schedule th a {
     text-decoration: none;
   }
   table.schedule td a:hover, table.schedule th a:hover {
     text-decoration: underline;
   }
   table.schedule tr.intro td {
     background-color: #c0c0ff;
   }
   table.schedule tr.phase1 td {
     background-color: #c0ffc0;
   }
   table.schedule tr.phase2 td {
     background-color: #ffffc0;
   }
   table.schedule tr.phase3 td {
     background-color: #ffe0c0;
   }
   table.schedule tr.phase4 td {
     background-color: #ffc0c0;
   }
   table.schedule th {
     font-weight: bold;
     text-align: center;
   }
   table.schedule td.title {
     text-align: center;
     font-weight: bold;
   }
    </style>
    <columns class="schedule">
        <column id="week"  title="Wk" />
        <column id="step"  title="Step" />
        <column id="tut"   title="Tutorial" />
        <column id="lab"   title="Lab" />
        <column id="lec"   title="Lecture" />
        <column id="other" title="Other" />
    </columns>
    <rows>

      <!-- Introduction -->
        <row semester="1" week="0" class="intro">
            <data for="week" span="6" class="title"><![CDATA[Introduction (<a href="{L_[Phase0]}">phase overview</a>)]]></data>
        </row>
